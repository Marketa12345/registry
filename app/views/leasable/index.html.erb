<h1>Přeměty volné k pronájmu</h1>


<h1>Listing Subjects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Description</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subjects.where(leasable:true).each do |subject| %>
      <tr>


        <td><%= subject.name %></td>
        <td><%= subject.type %></td>
        <td><%= subject.description %></td>
      
        <td><%= link_to 'Show', subject %></td>

 
      </tr>
    <% end %>
  </tbody>
</table>
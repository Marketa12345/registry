<h1>Přeměty volné k pronájmu</h1>



<table>
  <thead>
    <tr>
      <th>Jméno</th>
      <th>Typ</th>
      <th>Popis</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subjects.where(leasable:true).each do |subject| %>
      <tr>


        <td><%= subject.name %></td>
        <td><%= subject.type %></td>
        <td><%= subject.description %></td>
      
        <td><%= link_to 'Detail', subject %></td>

 
      </tr>
    <% end %>
  </tbody>
</table>